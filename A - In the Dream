#include <bits/stdc++.h>
using namespace std;
#define ll long long
#define vi vector<ll>
#define endl '\n'
#define yes cout << "YES" << endl
#define no cout << "NO" << endl
#ifdef LOCAL
#include "algo/debug.h"
#else
#define debug(...) 42
#endif
const double PI = acos(-1);
const ll MOD = 1e18;

void solve()
{
    // cout << fixed << setprecision(2);
    ll a, b, c, d, ok = 0;
    cin >> a >> b >> c >> d;
    if (max(a, b) <= 2 * min(a, b) + 2)
        ok = 1;
    if (ok && max(c - a, d - b) <= 2 * min(c - a, d - b) + 2) yes;
    else no;
}

int main()
{
    ios::sync_with_stdio(false);
    cin.tie(nullptr);
    ll tstcs;
    cin >> tstcs;
    for (int t = 1; t <= tstcs; t++)
    {
        // cout << "Case " << t << ": ";
        solve();
    }
    return 0;
}
